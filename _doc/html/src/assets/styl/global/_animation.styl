/* animate */   
@keyframes opacity { 
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
} 

@keyframes float {
	0% {
		transform: translatey(0px);
	}
	50% {
		transform: translatey(-5px);
	}
	100% {
		transform: translatey(0px);
	}
}

@keyframes float-left {
	0% {
		transform: translateX(0px);
	}
	50% {
		transform: translateX(3px);
	}
	100% {
		transform: translateX(0px);
	}
}
